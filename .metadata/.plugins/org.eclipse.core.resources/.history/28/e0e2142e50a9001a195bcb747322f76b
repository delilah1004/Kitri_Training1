package quiz.api;

import java.util.Scanner;

public class Quiz27 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("수를 입력하세요 : ");
		int a = sc.nextInt();
		System.out.print("수를 입력하세요 : ");
		int b = sc.nextInt();
		
		int sum = a+b;
		System.out.print("sum : " + sum);
		
	}
}